public class SelectionSort {
 
  public static void selectionSort(int[] list) {
    for (int i = 0; i < list.length - 1; i++) {
     
    int selMin = list[i];
      int selMinIndex = i;
      for (int j = i + 1; j < list.length; j++) { 
        if (selMin > list[j]) { //n * (n-2) times
          selMin = list[j];
          selMinIndex = j;
        }
      }

      
      if (selMinIndex != i) { 
        list[selMinIndex] = list[i];
        list[i] = selMin;
      }
    }
  }
}
